LIBRARY IEEE;
USE IEEE.STD_LOGIC_1164.ALL;
USE IEEE.STD_LOGIC_UNSIGNED.ALL;
ENTITY select211 IS
PORT(sel:IN STD_LOGIC;
	d0,d1:IN STD_LOGIC_VECTOR(7 DOWNTO 0);
		q:OUT STD_LOGIC_VECTOR(7 DOWNTO 0));
END select211;
ARCHITECTURE behave OF select211 IS
BEGIN
	PROCESS(sel)
	BEGIN
		CASE sel IS
		WHEN"0"=>q<=d0;
		WHEN"1"=>q<=d1;
		WHEN OTHERS=>NULL;
		END CASE;
	END PROCESS;
END behave;
